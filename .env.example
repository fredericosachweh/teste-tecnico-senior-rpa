# Environment variables
ENV=development
DEBUG=true

# Docker Compose: platform (optional). "make" auto-sets from OS: arm64/aarch64 -> linux/arm64, else linux/amd64.
# Uncomment and set if you run "docker compose" without make:
# DOCKER_PLATFORM=linux/arm64

# Database
DATABASE_URL=postgresql://app:app@localhost:5432/app

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# Selenium
HEADLESS=true
